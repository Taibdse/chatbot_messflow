<!DOCTYPE html>
<html lang="en">

<head>
  <link rel="stylesheet" href="bootstrap/css/bootstrap.min.css" id="bootstrap-css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <link rel="stylesheet" href="dist/css/AdminLTE.css">
  <link rel="stylesheet" href="dist/css/skins/_all-skins.min.css">
  <link rel="stylesheet" href="dist/css/user.css">
  <link rel="stylesheet" href="dist/css/fbchatpopup.css">

  <title>Chatbot Message</title>
  <style>
    .btn-send {
      background-color: green;
    }

    .btn-send,
    .btn-back,
    .btn-custom {
      padding-left: 25px;
      padding-right: 25px;
      border-radius: 5px;
    }
  </style>
</head>

<body class="hold-transition skin-blue sidebar-mini">

  <div class="wrapper">
    <% include ../partials/header %>
      <% include ../partials/menus %>
        <div class="content-wrapper">
          <div class="container">
            <div class="row">
              <div class="col-md-1"></div>
              <div class="col-md-10">
               
                <button class="btn btn-success pull-right" id="btnBroadcase">Broadcase</button>
                <br>
                <br>
                <table class="table table-striped table-hover table-condensed" id="tblBroadcase">
                  <!-- content here -->
                  
                </table>
              </div>
              <div class="col-md-1"></div>
            </div>
          </div>

          <!-- model -->
          <div class="modal fade" id="broadCaseDetail" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title">Message Information</h5>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <div class="modal-body">
                 <!-- content here -->
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-dark btn-custom" data-toggle="modal" data-target="#broadCaseDetail">Close</button>
                </div>
              </div>
            </div>
          </div>
          
        </div>
  </div>

  <!-- jQuery  -->
  <script src="plugins/jQuery/jquery-2.2.3.min.js"></script>
  <!-- bootstrap js -->
  <script src="bootstrap/js/bootstrap.min.js"></script>
  <!-- Broadcase custom js -->
  <script src="dist/js/pages/broadCase.js"></script>

</body>

</html>